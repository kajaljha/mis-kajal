<section class="panel">
  <header class="panel-heading"><h4>Consultant Details</h4></header>
<%= form_for [current_user, current_user.sales_consultants.build], :url =>  sales_consultant_create_user_sales_path(current_user) do |f| %>
     <div class=" col-lg-3">
        </div>
           <div class="panel-body">
              <div class=" form col-lg-9">
                  <form class="cmxform form-horizontal tasi-form" id="commentForm" method="get" action="">

                      <div class="form-group ">
                          <%= f.label :Name,:class=>"col-lg-2 control-label" %>
                               <div class="col-lg-9">
                                   <%= f.text_field :name ,:class=>"form-control", :required => true %><br />
                                </div>
                       </div>

                     <div class="form-group ">
                         <%= f.label :commission ,:class=>"col-lg-2 control-label" %>
                           <div class="col-lg-9">
                              <%= f.text_field :commission,:class=>"form-control", :required => true %><br />
                            </div>
                      </div>

                   <div class="form-group ">
                       <%= f.label :contact ,:class=>"col-lg-2 control-label" %>
                          <div class="col-lg-9 ">
                              <%= f.text_field :contact ,:class=>"form-control", :required => true %><br />
                          </div>
                    </div>
                  
                  <div class="form-group ">
                      <%= f.label :skype_id ,:class=>"col-lg-2 control-label"%>
                         <div class="col-lg-9">
                           <%= f.text_field :skype_id,:class=>"form-control", :required => true %><br />
                          </div>
                    </div>
    
                     <div class="form-group ">
                         <%= f.label :email_id,:class=>"col-lg-2 control-label"%>
                          <div class="col-lg-9">
                              <%= f.text_field :email_id ,:class=>"form-control", :required => true %><br />
                          </div>
                    </div>

                     <div class="form-group ">
                        <%= f.label :location, :class=>"col-lg-2 control-label"%>
                          <div class="col-lg-9">
                            <%= f.text_field :location ,:class=>"form-control", :required => true %><br />
                          </div>
                    </div>

                     <div class="form-group ">
                        <%= f.label :address,:class=>"col-lg-2 control-label"%>
                          <div class="col-lg-9">
                              <%= f.text_field :address,:class=>"form-control", :required => true %><br />
                          </div>
                    </div>

                   <div class="form-group "> 
                     <%= f.label :bank_account_no,:class=>"col-lg-2 control-label"%>
                        <div class="col-lg-9">
                           <%= f.text_field :bank_account_no ,:class=>"form-control", :required => true %><br />
                          </div>
                    </div>
    
                    <div class="form-group "> 
                       <%= f.label :bank_name,:class=>"col-lg-2 control-label"%>
                         <div class="col-lg-9">
                             <%= f.text_field :bank_name, :class=>"form-control", :required => true %><br />
                          </div>
                    </div>

                    <div class="form-group "> 
                        <%= f.label :branch_name,:class=>"col-lg-2 control-label"%>
                          <div class="col-lg-9">
                             <%= f.text_field :branch_name, :class=>"form-control", :required => true %><br />
                          </div>
                    </div>

                   <div class="form-group "> 
                       <%= f.label :ifsc_code ,:class=>"col-lg-2 control-label"%>
                          <div class="col-lg-9">
                              <%= f.text_field :ifsc_code, :class=>"form-control", :required => true %><br />
                          </div>
                    </div>

                    <div class="form-group">
                           <div class="col-lg-offset-2 col-lg-12">
                                <button class="btn btn-danger" type="submit">Save Consultant Details</button>

                           </div>
                    </div>
              </form>
           </div>
       </div>
    <% end %>
</section>



 
    